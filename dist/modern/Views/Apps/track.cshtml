    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title" id="exampleModalToggleLabel">Open Case</h4>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body p-2">
                <div class="row">
                    <div id="PriorityAddressFields">
                        <div class="col-md-12 mb-1">
                            <label for="example-select" class="form-label">Priority</label>
                            <select class="form-select" id="example-select">
                                <option>--Select--</option>
                                <option>High</option>
                                <option>Medium</option>
                                <option>Low</option>
                            </select>
                        </div>
                        <div class="col-md-12 mb-1">
                            <label for="inputlocation" class="form-label">Address</label>
                            <input type="text" class="form-control" id="inputlocation" placeholder="Location">
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-primary" data-bs-target="#exampleModalToggle2" data-bs-toggle="modal" data-bs-dismiss="modal">Save</button>
                <button type="button" class="btn btn-secondary waves-effect" data-bs-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>
<!--Second Modal-->
<div class="modal fade" id="exampleModalToggle2" aria-hidden="true" aria-labelledby="exampleModalToggleLabel2" tabindex="-1">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal-header bg-primary">
                <h5 class="modal-title text-white ml-auto" id="exampleModalToggleLabel2">Incident#: 126</h5>
                <h5 class="modal-title text-white ml-auto" id="exampleModalToggleLabel2">Time: Oct 20, 2020</h5>
                <h5 class="modal-title text-white" id="exampleModalToggleLabel2">Dubai <span><i class="mdi mdi-map-marker"></i></span></h5>
                @*<button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>*@
            </div>
            <div class="modal-body p-2">
                <div class="row">
                    <div id="PriorityAddressFields">
                        <div class="row">
                            <div class="col-md-4 mb-3">
                                <select class="form-select" id="example-select">
                                    <option>Insurance</option>
                                    <option>Yes</option>
                                    <option>No</option>
                                </select>
                            </div>
                            <div class="col-md-4 mb-3">
                                <select class="form-select" id="example-select">
                                    <option>Gender</option>
                                    <option>Male</option>
                                    <option>Female</option>
                                </select>
                            </div>
                            <div class="col-md-4 mb-3">
                                <select class="form-select" id="example-select">
                                    <option>High</option>
                                    <option>Medium</option>
                                    <option>Low</option>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-12 mb-3">
                            <input type="text" class="form-control" id="inputlocation" placeholder="Name">
                        </div>
                        <div class="col-md-12 mb-3">
                            <input type="number" class="form-control" id="inputlocation" placeholder="784-____-_______-__">
                        </div>
                        <div class="col-md-12 mb-3">
                            <input type="number" class="form-control" id="inputlocation" placeholder="784-____-_______-__">
                        </div>
                        <div class="col-md-12 mb-3">
                            <input type="text" class="form-control" id="inputlocation" placeholder="Policy No.">
                        </div>
                        <div class="col-md-12 modal-btm-ldng-wrapper">
                            <div class="card">
                                <div class="card-body text-center">
                                    <div class="modal-btm-ldng-inner-wrapper">
                                        <h4 class="header-title">Unfortunately,<br />something went wrong.</h4>
                                        <p class="sub-header font-13">
                                            We're trying to fix this.<br /><br />
                                            Reconnecting in 26 sec...
                                        </p>
                                        <div class="progress mb-2">
                                            <div class="progress-bar" role="progressbar" style="width: 33%" aria-valuenow="33" aria-valuemin="0" aria-valuemax="100"></div>
                                        </div>
                                    </div>
                                </div>
                            </div> 
                            <!-- end card-->
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button class="btn btn-primary" id="submitBtn" data-bs-target="#bottom-modal" data-bs-toggle="modal" data-bs-dismiss="modal">Submit</button>
                <button type="button" class="btn btn-secondary waves-effect" data-bs-dismiss="modal">Cancel</button>
            </div>
        </div>
    </div>
</div>


@section scripts {
    <!-- Plugins -->
    <script src="~/lib/flatpickr/flatpickr.min.js"></script>
    <script src="~/lib/apexcharts/apexcharts.min.js"></script>
    <script src="~/lib/selectize/js/standalone/selectize.min.js"></script>
    <script src="~/js/pages/dashboard-1.init.js"></script>
    <script src="~/lib/morris.js06/morris.min.js"></script>
    <script src="~/lib/raphael/raphael.min.js"></script>
    <script src="~/lib/jquery-sparkline/jquery.sparkline.min.js"></script>
    <script src="~/lib/admin-resources/jquery.vectormap/jquery-jvectormap-1.2.2.min.js"></script>
    <script src="~/lib/admin-resources/jquery.vectormap/maps/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/js/pages/dashboard-2.init.js"></script>
    <script src="~/js/pages/crm-opportunities.init.js"></script>
    <script>
        $(document).ready(function () {
            $("#dashboardBtn").click(function () {
                $("#totalIncidentTile").toggle();
            });
            $("#submitBtn").click(function () {
                $("#IncidentPersonWrapper").show();
            });
            $(".single-person-wrapper").click(function () {
                $(".chat-control-wrapper").toggle();
            });
            $("#ChatControlCloseBtn").click(function () {
                $(".chat-control-wrapper").hide();
            });
        });
    </script>
}
